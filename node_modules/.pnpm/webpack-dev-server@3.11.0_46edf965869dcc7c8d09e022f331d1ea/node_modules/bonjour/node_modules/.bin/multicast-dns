#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Volia/Documents/git/ProjetBDDS3/node_modules/.pnpm/multicast-dns@6.2.3/node_modules/multicast-dns/node_modules:/mnt/c/Users/Volia/Documents/git/ProjetBDDS3/node_modules/.pnpm/multicast-dns@6.2.3/node_modules:/mnt/c/Users/Volia/Documents/git/ProjetBDDS3/node_modules/.pnpm/node_modules:/mnt/c/Users/Volia/Documents/git/ProjetBDDS3/node_modules:/mnt/c/Users/Volia/Documents/git/node_modules:/mnt/c/Users/Volia/Documents/node_modules:/mnt/c/Users/Volia/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Volia/Documents/git/ProjetBDDS3/node_modules/.pnpm/bonjour@3.5.0/node_modules/bonjour/node_modules:/mnt/c/Users/Volia/Documents/git/ProjetBDDS3/node_modules/.pnpm/bonjour@3.5.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Volia/Documents/git/ProjetBDDS3/node_modules/.pnpm/multicast-dns@6.2.3/node_modules/multicast-dns/node_modules:/mnt/c/Users/Volia/Documents/git/ProjetBDDS3/node_modules/.pnpm/multicast-dns@6.2.3/node_modules:/mnt/c/Users/Volia/Documents/git/ProjetBDDS3/node_modules/.pnpm/node_modules:/mnt/c/Users/Volia/Documents/git/ProjetBDDS3/node_modules:/mnt/c/Users/Volia/Documents/git/node_modules:/mnt/c/Users/Volia/Documents/node_modules:/mnt/c/Users/Volia/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Volia/Documents/git/ProjetBDDS3/node_modules/.pnpm/bonjour@3.5.0/node_modules/bonjour/node_modules:/mnt/c/Users/Volia/Documents/git/ProjetBDDS3/node_modules/.pnpm/bonjour@3.5.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../multicast-dns@6.2.3/node_modules/multicast-dns/cli.js" "$@"
else
  exec node  "$basedir/../../../../../multicast-dns@6.2.3/node_modules/multicast-dns/cli.js" "$@"
fi
